<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >
<web-app>
  <display-name>WebProxyServer1.0</display-name>
  
  <servlet>
   <servlet-name>Proxystart</servlet-name>
   <servlet-class>org.littleshoot.proxy.start.ProxyServerStartServlet</servlet-class>
   <init-param>
    <param-name>--port</param-name>
    <param-value>9999</param-value>
   </init-param>
   <load-on-startup>1</load-on-startup>
</servlet>

</web-app>
